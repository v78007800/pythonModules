    "import codecs\n",
    "import re\n",
    "pattern = r'(?:[A-Z])+'\n",
    "abbr=[]\n",
    "f = codecs.open('d.txt', encoding='utf-8')\n",
    "for line in f:\n",
    "    abbr.extend(re.findall(pattern, repr(line)))\n",
    "print(abbr)"
